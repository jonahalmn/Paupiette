<p id="notice"><%= notice %></p>

<h1>Repas</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Menu</th>
      <th>Location</th>
      <th>Organisator</th>
      <th>Participants</th>
      <th>Participantnumber</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @repas.each do |repa| %>
      <tr>
        <td><%= repa.Date %></td>
        <td><%= repa.menu %></td>
        <td><%= repa.location %></td>
        <td><%= repa.organisator %></td>
        <td><%= repa.participants %></td>
        <td><%= repa.participantnumber %></td>
        <td><%= link_to 'Show', repa %></td>
        <td><%= link_to 'Edit', edit_repa_path(repa) %></td>
        <td><%= link_to 'Destroy', repa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Repa', new_repa_path %>
