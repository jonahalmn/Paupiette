<%= simple_form_for(@repa) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :Date %>
    <%= f.input :menu %>
    <%= f.input :location %>
    <%= f.input :organisator %>
    <%= f.input :participants %>
    <%= f.input :participantnumber %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
